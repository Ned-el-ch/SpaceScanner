<h1>Welcome</h1>
<% if logged_in? %>
    <h1>You are Logged In, <%= current_user.name %></h1>
<%end%>

<% if !logged_in?%>

    <%= button_to "Log In", '/login', method: :get%>
    <%= button_to "Sign Up", '/users/new', method: :get%>

<% else%>

    <%= button_to "Log Out", '/logout', method: :post%>

<% end%>